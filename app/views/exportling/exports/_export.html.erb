<tr>
  <td><%= export.id %></td>
  <td><%= export.name %></td>
  <td><%= export.file_name %></td>
  <td><%= export.file_type %></td>
  <td><%= export.created_at %></td>
  <td><%= export.updated_at %></td>
  <td><%= export.status.capitalize %></td>
  <td><% if export.completed? %>
    <%= link_to 'Download', download_export_path(export) %><% end %>
   <% if export.failed? %>
    <%= link_to 'Retry', retry_export_path(export) %><% end %>
  </td>
</tr>
